INPUT "Ingrese dos numeros: "; num1
INPUT num2

PRINT "La suma de "; num1; " + "; num2; "es igual a "; num1 + num2
PRINT "La resta de "; num1; " - "; num2; "es igual a "; num1 - num2
PRINT "La multiplicacion de "; num1; " x "; num2; "es igual a "; num1 * num2

IF num2 = 0 THEN
    PRINT "ERROR"
ELSE
    PRINT "La division de"; num1; " % "; num2; "es igual a "; num1 / num2
END IF


