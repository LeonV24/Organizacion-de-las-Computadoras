INPUT "Ingrese un numero del 1-4"; num

SELECT CASE num

    CASE 1
        PRINT "El numero coincide con la verano"
    CASE 2
        PRINT "El numero coincide con la otonio"
    CASE 3
        PRINT "El numero coincide con la invierno"
    CASE 4
        PRINT "El numero coincide con la primavera"

END SELECT
