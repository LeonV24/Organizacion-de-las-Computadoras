INPUT "Ingrese un numero del 1-7"; num

SELECT CASE num

    CASE 1
        PRINT "El codigo coincide con el produto RTX 2080 con un valor de 1.50"

    CASE 2
        PRINT "El codigo coincide con el produto RTX 3080 con un valor de 2.00"

    CASE 3
        PRINT "El codigo coincide con el produto RTX 4080 con un valor de 2.50"


END SELECT

